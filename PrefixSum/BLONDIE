#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    long long int n;
	    cin>>n;
	    
	    long long e;
	    
	    long long prefix[n];
	    memset(prefix , 0 ,sizeof(prefix));
	    
	    for(int i=0;i<n;i++)
	    {
	        cin>>e;
	        
	        if(i==0)
	        {
	            prefix[i]=e;
	        }
	        
	        else
	        {
	            	        
	            if(e==-1) 
	            {
	            
	                e = prefix[i-1]/i;
	                prefix[i]=prefix[i-1]+e;
	            }
	        
	            else 
	                prefix[i]=prefix[i-1]+e;
	            
	        
	        }
	        
	        cout<<e<<" ";
	        
	    }
	    
	    cout<<endl;
	}
	return 0;
}
