#include <bits/stdc++.h>
 
using namespace std;
 
int main() {
	int n;
	cin >> n;										
	while(n--)
	{
		string s;
		cin>>s;
		int i=0;
		int ns=0,nsj=0;
		while(i<s.length())
		{
			i=s.find("SUVO",i);
			if(i==-1)
			{
				break;
			}
 
			int t=i;
			
			i=i+4;
			if(i>=0 && (i+2)<s.length())
			{
 
				if(s[i]=='J' && s[i+1]=='I' && s[i+2]=='T')
				{
					nsj++;
					i=i+3;
				}				
			}
 
			if(i==(t+4))
			{
				ns++;
			}
		}
		cout<<"SUVO = "<<ns<<", SUVOJIT = "<<nsj<<endl;
	}		
}
