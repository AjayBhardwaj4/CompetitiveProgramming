#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	
	vector<int> v;
	int inp;
	for(int i=0;i<n;i++)
	{
		cin>>inp;
		v.push_back(inp);
	}
	
	int q;
	
	int prefix[v.size()];
	
	prefix[0]=v[0];
	for(int i=1;i<n;i++)
	{
		prefix[i]=prefix[i-1]+v[i];
	}
	
	vector<pair<int,int>> v1;
	
	cin>>q;
	
	int l,r;
	for(int i=0;i<q;i++)
	{
		cin>>l>>r;
	    if(l>0)	cout<<(prefix[r]-prefix[l-1])<<endl;
		else cout<<prefix[r]<<endl;
	}
	
//	for(int i=0;i<q;i++)
//	{
	   // cout<<(prefix[v1[i].second]-prefix[v1[i].first-1])<<endl;
//	}
	
	return 0;
}
